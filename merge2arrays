#include <stdio.h>

int main()
{
    int i,n,j,m,array1[20],array2[20],array3[20];
    printf("enter the number of elements for 1st array");
    scanf("%d",&n);
    printf("enter the elements for first array");
    for (i=0;i<n;i++)
    {
        scanf("%d",&array1[i]);
    }
    printf("enter the number of elements for  2nd array");
    scanf("%d",&m);
    printf("enter the elements for 2nd array");
    for (j=0;j<m;j++)
    {
        scanf("%d",&array2[j]);
    }
     for (j=0;j<m;j++)
     {
         array1[n+j]=array2[j];
     }
     
    for (i=0;i<n+m;i++)
    {
        printf("%d",array1[i]);
    }
    

    return 0;
}
