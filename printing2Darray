#include <stdio.h>

int main()
{
    int i,j,n,m;
    printf("enter the number of rows");
    scanf("%d",&m);
    printf("enter the number of columns:");
    scanf("%d",&n);
    int array[m][n];
    printf("enter the elements:\n");
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
            {
                scanf("%d",&array[i][j]);
            }
    }
    printf("the matrix is:\n");
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
            {
                printf("%d ",array[i][j]);
            }
            printf("\n");
    }
    
    return 0;
}
