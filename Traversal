#include <stdio.h>

int main()
{
    int i,n,array[20];
    printf("enter the number of elements for  array");
    scanf("%d",&n);
    printf("enter the elements for array");
    for (i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
        
    }
    printf("\nthe array is:");
    for (i=0;i<n;i++)
    {
    printf("\n array[%d]=%d ",i,array[i]);
    }

    return 0;
}
